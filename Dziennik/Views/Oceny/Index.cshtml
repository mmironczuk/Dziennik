@model IEnumerable<Dziennik.Models.Ocena>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.wartosc)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.opis)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.koncowa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nauczanie)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.wartosc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.opis)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.koncowa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nauczanie.NauczanieId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.OcenaId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.OcenaId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.OcenaId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
